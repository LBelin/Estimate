<center>
<h1>Listing features</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Estimate</th>
      <th>Actual</th>
      <th>Num cases</th>
      <th>Completed?</th>
      <th>Percentage</th>
      <th>Active?</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @features.each do |feature| %>
      <tr>
        <td><%= link_to feature.title, feature %></td>
        <td><%= feature.estimate %></td>
        <td><%= feature.actual %></td>
        <td><%= feature.num_cases %></td>
        <td><%= feature.completed? %></td>
        <td> Calc Percentage </td>
        <td><%= feature.active? %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

Create a feature:<%=render 'form'%>
</center>
